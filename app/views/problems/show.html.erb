<div class="card mb-3">
    <% @problem.solutions.each do  |solution| %>
    <div class="card-body">
    <h5 class="card-title"><%= solution.title %></h5>
    <p class="card-text"><%= solution.description %></p>
    <p class="card-text"><small class="text-muted"><%= solution.user.name %></small></p>
    <%= link_to 'More details...', solution_path(solution) %>
  </div>
  <% end %>
</div>
