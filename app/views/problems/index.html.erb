<%= render 'search' %>
<%= render 'filters' %>
<div class="wrapper">
  <div class="container">
    <div class="row">
      <% @problems.each do |problem| %>
      <div class="col-md-4">
        <%= link_to(problem_path(problem)) do %>
        <div class="pb-card w-100 bg-white h-95 hovering">
          <%= render 'shared/problem_card', problem: problem %>
          <% end %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>


